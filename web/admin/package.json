{
  "name": "antapi",
  "version": "0.0.1",
  "private": true,
  "description": "ðŸš€ Open source Golang Headless CMS to easily build customisable APIs",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.12.1",
    "@ant-design/pro-descriptions": "^1.7.10",
    "@ant-design/pro-form": "^1.24.1",
    "@ant-design/pro-layout": "^6.18.2",
    "@ant-design/pro-list": "^1.7.5",
    "@ant-design/pro-table": "^2.37.1",
    "@umijs/openapi": "^1.1.17",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/route-utils": "^1.0.37",
    "antd": "^4.15.6",
    "classnames": "^2.3.1",
    "form-render": "^1.3.4",
    "fr-generator": "^2.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-use": "^17.2.4",
    "tailwindcss": "1.9.4",
    "umi": "^3.4.22",
    "umi-request": "^1.3.6"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.4",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@umijs/fabric": "^2.5.13",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-ant-design-pro": "^1.3.1",
    "@umijs/preset-dumi": "^1.1.18",
    "@umijs/preset-react": "^1.8.8",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.26.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "postcss-import": "12.0.1",
    "prettier": "^2.3.0",
    "puppeteer-core": "^9.1.1",
    "stylelint": "^13.13.1",
    "typescript": "^4.2.4",
    "webpack-plugin-fr-theme": "^0.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
